{
  "Effect Pattern": {
    "prefix": "effect",
    "body": [
      "effect ${1:pattern syntax}:",
      "\twhen triggered:",
      "\t\t${2:@intrinsic(\"print\", \"Hello\")}"
    ],
    "description": "Define an effect pattern (side effects, no return value)"
  },
  "Expression Pattern": {
    "prefix": "expression",
    "body": [
      "expression ${1:pattern syntax}:",
      "\tget:",
      "\t\t${2:@intrinsic(\"add\", a, b)}"
    ],
    "description": "Define an expression pattern (returns a value)"
  },
  "Expression Pattern with Setter": {
    "prefix": "expressionset",
    "body": [
      "expression ${1:pattern syntax}:",
      "\tget:",
      "\t\t${2:@intrinsic(\"load\", var)}",
      "\tset:",
      "\t\t${3:@intrinsic(\"store\", var, value)}"
    ],
    "description": "Define an expression pattern with both getter and setter"
  },
  "Section Pattern": {
    "prefix": "section",
    "body": [
      "section ${1:pattern syntax}:",
      "\twhen triggered:",
      "\t\t${2:@intrinsic(\"execute\", section)}"
    ],
    "description": "Define a section pattern (block with indented code)"
  },
  "Condition Pattern": {
    "prefix": "condition",
    "body": [
      "condition ${1:pattern syntax}:",
      "\twhen triggered:",
      "\t\t${2:@intrinsic(\"branch\", cond)}"
    ],
    "description": "Define a condition pattern"
  },
  "Legacy Pattern Definition": {
    "prefix": "pattern",
    "body": [
      "pattern:",
      "\tsyntax: ${1:pattern_syntax}",
      "\twhen triggered:",
      "\t\t${2:@intrinsic(\"print\", \"Hello\")}"
    ],
    "description": "Define a new pattern with legacy syntax"
  },
  "Pattern with Priority": {
    "prefix": "patternp",
    "body": [
      "pattern:",
      "\tsyntax: ${1:pattern_syntax}",
      "\tpriority: ${2:1}",
      "\twhen triggered:",
      "\t\t${3:@intrinsic(\"print\", \"Hello\")}"
    ],
    "description": "Define a new pattern with explicit priority"
  },
  "Pattern with Parsed": {
    "prefix": "patternc",
    "body": [
      "pattern:",
      "\tsyntax: ${1:pattern_syntax}",
      "\twhen parsed:",
      "\t\t${2:# compile-time behavior}",
      "\twhen triggered:",
      "\t\t${3:@intrinsic(\"print\", \"Hello\")}"
    ],
    "description": "Define a pattern with compile-time and runtime behavior"
  },
  "Set Variable": {
    "prefix": "set",
    "body": "set ${1:variable} to ${2:value}",
    "description": "Set a variable to a value"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if {${1:condition}}:",
      "\t${2:body}"
    ],
    "description": "If statement with lazy condition"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "loop while {${1:condition}}:",
      "\t${2:body}"
    ],
    "description": "While loop with lazy condition"
  },
  "Loop N Times": {
    "prefix": "loop",
    "body": [
      "loop ${1:5} times:",
      "\t${2:body}"
    ],
    "description": "Loop a fixed number of times"
  },
  "Loop From To": {
    "prefix": "loopfrom",
    "body": [
      "loop from ${1:0} to ${2:10}:",
      "\t${3:body}"
    ],
    "description": "Loop from start to end value"
  },
  "Import": {
    "prefix": "import",
    "body": "import ${1:lib/prelude.3bx}",
    "description": "Import a library file"
  },
  "Intrinsic Call": {
    "prefix": "@intrinsic",
    "body": "@intrinsic(\"${1:operation}\", ${2:args})",
    "description": "Call an intrinsic operation"
  },
  "Intrinsic Print": {
    "prefix": "@print",
    "body": "@intrinsic(\"print\", ${1:value})",
    "description": "Print a value to console"
  },
  "Intrinsic Store": {
    "prefix": "@store",
    "body": "@intrinsic(\"store\", ${1:variable}, ${2:value})",
    "description": "Store a value in a variable"
  },
  "Intrinsic Load": {
    "prefix": "@load",
    "body": "@intrinsic(\"load\", ${1:variable})",
    "description": "Load a value from a variable"
  },
  "Intrinsic Add": {
    "prefix": "@add",
    "body": "@intrinsic(\"add\", ${1:a}, ${2:b})",
    "description": "Add two values"
  },
  "Intrinsic Subtract": {
    "prefix": "@sub",
    "body": "@intrinsic(\"sub\", ${1:a}, ${2:b})",
    "description": "Subtract two values"
  },
  "Intrinsic Multiply": {
    "prefix": "@mul",
    "body": "@intrinsic(\"mul\", ${1:a}, ${2:b})",
    "description": "Multiply two values"
  },
  "Intrinsic Divide": {
    "prefix": "@div",
    "body": "@intrinsic(\"div\", ${1:a}, ${2:b})",
    "description": "Divide two values"
  },
  "Intrinsic FFI Call": {
    "prefix": "@call",
    "body": "@intrinsic(\"call\", \"${1:library}\", \"${2:function}\", ${3:args})",
    "description": "Call an external function via FFI"
  },
  "Intrinsic Evaluate": {
    "prefix": "@evaluate",
    "body": "@intrinsic(\"evaluate\", ${1:lazyExpression})",
    "description": "Evaluate a lazy expression"
  },
  "Intrinsic Execute": {
    "prefix": "@execute",
    "body": "@intrinsic(\"execute\", section)",
    "description": "Execute a section block"
  },
  "Lazy Expression": {
    "prefix": "lazy",
    "body": "{${1:expression}}",
    "description": "Create a lazy expression (evaluated on demand)"
  },
  "Print Statement": {
    "prefix": "print",
    "body": "print ${1:value}",
    "description": "Print a value to the console"
  },
  "Comment": {
    "prefix": "#",
    "body": "# ${1:comment}",
    "description": "Single line comment"
  },
  "Comment Block": {
    "prefix": "##",
    "body": [
      "# ============================================================",
      "# ${1:Section Title}",
      "# ============================================================"
    ],
    "description": "Comment block for section headers"
  }
}
