{
    "name": "3BX Dark Modern",
    "type": "dark",
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "scope": "string",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6A9955"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#D4D4D4"
            }
        }
    ],
    "semanticTokenColors": {
        "effect": "#4FC1FF",
        "expression": "#DCDCAA",
        "section": {
            "foreground": "#4EC9B0",
            "fontStyle": "bold"
        },
        "pattern": {
            "foreground": "#DCDCAA",
            "fontStyle": "italic"
        },
        "function": "#DCDCAA",
        "variable": "#9CDCFE",
        "string": "#CE9178",
        "number": "#B5CEA8",
        "comment": "#6A9955",
        "operator": "#D4D4D4"
    }
}
